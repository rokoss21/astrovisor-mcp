{
  "name": "astrovisor-mcp",
  "version": "4.1.0",
  "description": "AstroVisor MCP server that auto-syncs tools from the AstroVisor OpenAPI schema (full endpoint coverage).",
  "main": "build/index.js",
  "bin": {
    "astrovisor-mcp": "build/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "start": "node build/index.js",
    "dev": "npm run build && npm start",
    "start:http": "node build/http-server.js",
    "start:jsonrpc": "node build/mcp-http-server.js",
    "test": "node test-production.cjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "astrovisor",
    "astrology",
    "openapi",
    "claude",
    "ai"
  ],
  "author": "Emil Rokossovskiy <emil@astrovisor.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokoss21/astrovisor-mcp.git"
  },
  "homepage": "https://astrovisor.io",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ]
}
